<?xml version="1.0" encoding="UTF-8" ?>
<rss version="2.0">
    <channel>
        <title>{{ feed_info.title }}</title>
        <description>{{ feed_info.desc }}</description>
        <link>{{ feed_info.link }}</link>
        <lastBuildDate>{{ feed_info.updated }}</lastBuildDate>
    </channel>

    {% for item in feed_data %}
        <item>
            <title>{{ item.title }}</title>
            <description>{{ item.desc }}</description>
            <link>{{ item.where }}</link>
            <pubDate>{{ item.when.strftime('%a, %d %b %Y %H:%M:%S %Z') }}</pubDate>
        </item>
    {% endfor %}
</rss>
